    "typedoc": "^0.22.15",
    "typedoc-plugin-extras": "^2.2.3",
    "typedoc-plugin-markdown": "^3.12.1",
    "typedoc-plugin-missing-exports": "^0.23.0",
    "typescript": "^4.5.4",
    "webpack": "^5.38.1",
    "webpack-bundle-analyzer": "^4.4.2",
