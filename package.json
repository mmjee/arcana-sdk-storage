{
  "name": "@arcana/storage",
  "version": "0.3.7",
  "description": "Upload, Download, Share can be performed using this SDK",
  "main": "dist/index.js",
  "browser": "dist/index.js",
  "unpkg": "dist/standalone/storage.umd.js",
  "jsdelivr": "dist/standalone/storage.umd.js",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "build": "rimraf dist types && npm run build:ts && npm run build:web",
    "build:web": "webpack",
    "build:ts": "tsc --project tsconfig.json",
    "prettier": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"  ",
    "docs": "typedoc --options typedoc.json",
    "htmldocs": "typedoc --options typedochtml.json",
    "lint": "eslint 'src/**/*.ts' 'src/**/*.js'",
    "prepare": "npm run build",
    "prepublish": "npm run build",
    "postversion": "git push && git push --tags",
    "test": "c8 ava",
    "integration-test": "mocha 'integration-tests/**/*.ts' --recursive --timeout 3000000 -R spec --ui bdd",
    "build:web:watch": "npm run build:web -- --watch --progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arcana-network/storage-sdk.git"
  },
  "keywords": [],
  "author": "Saurav Kanchan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/arcana-network/storage/issues"
  },
  "homepage": "https://github.com/arcana-network/storage#readme",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.6",
    "@peculiar/webcrypto": "^1.3.2",
    "@types/mocha": "^9.1.1",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "ava": "^4.1.0",
    "blob-polyfill": "^7.0.20220408",
    "browser-env": "^3.3.0",
